<script type="text/javascript">
$(function() {
	var form = $('#createSearchForm');
	var searchField = form.find('input[name="query"]');
	var url = form.attr('action');
	form.find(':input').keypress(function(e) {
		if (e.keyCode == 13) {
			var contentDiv = $(this).closest('.tab_content');
			var postData = contentDiv.find('form').serialize();
			var addon = searchField.val();
			$('#Create').myTabs({
				'load' : url + '?query=' + addon,
				post : postData
			});
		}
	});
	$('#jquery-colour-picker ul').on('click', 'li', function(e) {
		e.preventDefault();
		var colorId = $(this).attr("id");
	});
	$('select.medium2').selectbox({
		speed : 200,
		effect : 'slide',
		classHolder : 'sbHolderMedium2'
	});
	$('#category_select').on('change', function(e) {
		var contentDiv = $(this).closest('.tab_content');
		var postData = contentDiv.find('form').serialize();
		var addon = $(this).val();
		if (addon == 0)
			return;
		$('#Create').myTabs({
			'load' : url + '?category=' + addon,
			post : postData
		});
	});
	$.widget("custom.catcomplete", $.ui.autocomplete, {
		_renderMenu : function(ul, items) {
			var that = this, currentCategory = "";
			$.each(items, function(index, item) {
				if (item.category != currentCategory) {
					ul.append("<li class='ui-autocomplete-category'>" + item.category + "</li>");
					currentCategory = item.category;
				}
				that._renderItemData(ul, item);
			});
		},
		_renderItem : function(ul, item) {
			return $("<li></li>").data("item.autocomplete", item).append("<a>" + item.formatted + "</a>").appendTo(ul);
		}
	});
	searchField.bind("keydown", function(event) {
		if (event.keyCode === $.ui.keyCode.TAB && $(this).data("ui-autocomplete").menu.active) {
			event.preventDefault();
		}
	}).catcomplete({
		source : function(request, response) {
			$.getJSON("/autocomplete", {
				query : request.term,
				itemOrigin : 'product'
			}, response);
		},
		focus : function() {
			return false;
		},
		select : function(a, b) {
			$(this).val(b.item.value);
			var contentDiv = $(this).closest('.tab_content');
			var postData = contentDiv.find('form').serialize();
			var addon = searchField.val();
			$('#Create').myTabs({
				'load' : url + '?query=' + addon,
				post : postData
			});
		}
	});
});

var itemType = 1;	// 상품type
var curPage = 0;

// 상품 리스트를 가져온다
function lastPostFunc(goPage){
	curPage = goPage-1;
	chkGet = false;
	
	var postData = new Object();

	var shopstyle_api_key = '<%=SHOPSTYLE_KEY%>';
	var per_page = 30;
	var searchKey = $('#category').val();
	
	if(itemType!=3 && searchKey==""){
		$('#itemList').html("");
		$('#pagingDv').html("");
	} else {
		var api_url = '';
		var method_type = 'get';

		if(itemType==1){	// shop style 
			method_type = 'get';
			api_url = 'http://api.shopstyle.com/api/v2/products?pid='+shopstyle_api_key
					+ '&fts='+encodeURIComponent(searchKey)
					+ '&offset='+(curPage*per_page)
					+ '&limit='+per_page;
			
		} else if(itemType==2){	// DB
			method_type = 'post';
			//api_url = '/products/productListCallback?page='+(curPage+1)+'&per_page='+per_page;
			api_url = '/products/productListCallback';
			
			postData.page = curPage+1;
			postData.cate_code = $('#category').val();
			postData.per_page = 30;
			
		} else if(itemType==3){	// My items
			method_type = 'post';
			//api_url = '/mypage/itemListCallback?page='+(curPage+1)+'&type=P&per_page='+per_page;
			api_url = '/products/myProductListCallback';
			
			postData.page = curPage+1;
			postData.type = 'P';
			//postData.searchKey = $('#category').val();
			postData.per_page = 30;
		}
		
		// ajax post 에서 세션 유지 시키기 위함
		$.ajaxSetup({
		  beforeSend: function(xhr) {
		    xhr.setRequestHeader('X-CSRF-Token', $('meta[name="csrf-token"]').attr('content'));
		  }
		});
		
		$.ajax({
		    url     : api_url,
		    type    : method_type,
		    dataType: 'json',
		    data	: $.param(postData),
		    async   : false,
		    success : function(json){
		    	products = json.products;
		    	$('#itemList').html("");
		    	if(products.length>0) {
					curPage++;
					$.each(products, function (index, product) {
						var item_api = "";
						var item_img_url1 = "";
						var item_img_url2 = "";
						var item_url = "";
						var item_id = "";
						var file_path = "<%= request_host%>/data/product/";	// 상품 이미지 폴더
						var img_directory1 = "original/"
						var img_directory2 = "medium/"
						
						if(itemType==1){	// shop style
							oCollection.productURL = "";	// 상품 이미지 폴더
							oCollection.setURL = "";	// 꾸민 이미지 폴더
							oCollection.productStyleDirectory = "";
							
							item_api = "shopstyle";
							item_id = product.id;
							item_img_url1 = product["image"]["sizes"]["IPhone"]["url"];
							item_img_url2 = product["image"]["sizes"]["Large"]["url"];
							item_url = product.clickUrl;
						} else if(itemType==2){	// DB
							oCollection.productURL = "<%= request_host%>/data/product/";	// 상품 이미지 폴더
							oCollection.setURL = "<%= request_host%>/data/product/";	// 상품 이미지 폴더
							oCollection.productStyleDirectory = "/original/";
							
							item_api = "";
							item_id = product.id;
							item_img_url1 = product.img_file_name;
							item_img_url2 = file_path+img_directory2+product.img_file_name;
							item_url = product.url;
						} else if(itemType==3){	// My items
							oCollection.productURL = "<%= request_host%>/data/product/";	// 상품 이미지 폴더
							oCollection.setURL = "<%= request_host%>/data/product/";	// 상품 이미지 폴더
							oCollection.productStyleDirectory = "/original/";
							
							item_api = "";
							item_id = product.id;
							item_img_url1 = product.img_file_name;
							item_img_url2 = file_path+img_directory2+product.img_file_name;
							item_url = product.url;
						}
						
						var li = ''
						+ '<li class="ui-draggable" style="border:1px solid #CCCCCC;text-align:center;vertical-align:middle">'
						+ '	<img src='+item_img_url2+'>'
						+ '	<input type="hidden" name="itemOrigin" value="product">'
						+ '	<input type="hidden" name="itemAPI" value="'+item_api+'">'
						+ '	<input type="hidden" name="itemURL" value="'+item_url+'">'
						+ '	<input type="hidden" name="itemId" value="'+item_id+'">'
						+ '	<input type="hidden" name="itemImg" value="'+item_img_url1+'">'
						+ '</li>';
	
						$('#itemList').append(li);
					});
					//$('#loadingDiv').hide();
					
					// 페이징
					var pageStr = '' 
					+ '<ul class="pagging basic">'
					+ '<li><a class="singleBack btl bbl" href="javascript:goPage(1)"></a></li>';
					
					for(i=1;i<=5;i++){
						if (curPage==i){
							pageStr += '<li class="active"><a href="javascript:goPage('+i+')">'+i+'</a></li>';
						} else {
							pageStr += '<li><a href="javascript:goPage('+i+')">'+i+'</a></li>';
						}
					}
					pageStr += ''
					+ '	<li>'
					+ '		<a class="singleForward" href="javascript:goPage(5)"></a>'
					+ '	</li>'
					+ '	<div class="clear"></div>'
					+ '</ul>';
					
					$('#pagingDv').html(pageStr);
		    	} else {
		    		//$('#loadingDiv').hide();
		    	}
		    },
		    error   : function(e){
		        //$('#loadingDiv').hide();
		    }
		});
	}
}

// 메뉴선택
function changeType(val){
	beforeLeave(false);
	curPage = 0;
	itemType = val;
	
	for(i=1;i<=3;i++){
		$("#menu"+i).removeClass();
		$("#menu"+i).addClass("publish bGray small");
	}
	$("#menu"+val).removeClass();
	$("#menu"+val).addClass("publish bBlue small");
	$('#itemList').html("");
	$('#pagingDv').html("");
	
	var selectStr = ''
	if(val==3){
		lastPostFunc(1);
	} else {
		selectStr = ''
		+ '<select id="category" class="medium2" onchange="lastPostFunc(1)">'
		+ '	<option value="">Choose category</option>'
		+ '	<option value="Women">Women</option>'
		+ '	<option value="Bags">Bags</option>'
		+ '	<option value="Shoes">Shoes</option>'
		+ '	<option value="Beauty">Beauty</option>'
		+ '	<option value="Jewelry">Jewelry</option>'
		+ '	<option value="Kids">Kids</option>'
		+ '	<option value="Home">Home</option>'
		+ '	<option value="Men">Men</option>'
		+ '</select>';
	}
	$('#selectDv').html(selectStr);
	
	
}

function goPage(val){
	lastPostFunc(val);
}

